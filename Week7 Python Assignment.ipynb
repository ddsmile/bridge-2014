      "from bs4 import BeautifulSoup\n",
      "import requests\n",
      "url = 'http://www.numpy.org/'\n",
      "r  = requests.get(url)\n",
      "data = r.text\n",
      "soup = BeautifulSoup(data)\n",
      "for link in soup.find_all('a'):\n",
      "    http = link.get('href')\n",
      "    if http.startswith('http'):\n",
      "        print http"
